<div class="container">
  <h1>All Questions</h1>
  <h2>Total Questions: <%= @total_questions %></h2>
  <h3><a href="/questions/new">Ask a Question</a></h3>
  <div class="question">
<% @questions.each do |question| %>
<%= erb :'questions/_question', layout: false, locals: { question: question } %>
<% end %>
  </div>
</div>
